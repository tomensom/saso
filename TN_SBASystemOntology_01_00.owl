<?xml version="1.0"?>
<rdf:RDF xmlns="http://tomensom.com/ontology/sba-system-ontology#"
     xml:base="http://tomensom.com/ontology/sba-system-ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:saso1="https://github.com/tomensom/saso#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:saso="http://tomensom.com/saso#">
    <owl:Ontology rdf:about="http://tomensom.com/saso">
        <dc:title xml:lang="en">Software-based Artwork Structure Ontology</dc:title>
        <rdfs:label xml:lang="en">Software-based Artwork Structure Ontology</rdfs:label>
        <skos:altLabel xml:lang="en">SASO</skos:altLabel>
        <terms:contributor>Tom Ensom</terms:contributor>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/contributor"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tomensom.com/saso#hasConstituent -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasConstituent">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Realisation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tomensom.com/saso#SoftwareSuperObject"/>
                    <rdf:Description rdf:about="http://tomensom.com/saso#TechnicalEnvironment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>A Realisation is made up of one or more Software Super-Object and one or more Technical Environment.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasDataComponent -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasDataComponent">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#SoftwareSuperObject"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Data"/>
        <rdfs:comment>A Software Super-Object can consist of one or more Data components.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasHardwareComponent -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasHardwareComponent">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#HardwareEnvironment"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Hardware"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A Hardware Environment can consist of one or more Hardware components.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasInterface -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasInterface">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Hardware"/>
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Hardware"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A Software or Hardware component may use a Software or Hardware component to communicate with another Software and Hardware component.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasRealisation -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasRealisation">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Artwork"/>
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Variant"/>
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Version"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Realisation"/>
        <rdfs:comment>An Artwork, Version or Variant may have one or more Realisation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasSoftwareComponent -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasSoftwareComponent">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#SoftwareEnvironment"/>
        <rdfs:domain rdf:resource="http://tomensom.com/saso#SoftwareSuperObject"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A Software Super-Object or Software Environment can consist of one or more Software component.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasVariant -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasVariant">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Artwork"/>
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Version"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Variant"/>
        <rdfs:comment>An Artwork or Version may have one or more Variant.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hasVersion -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hasVersion">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Artwork"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#Version"/>
        <rdfs:comment>An Artwork may have one or more Version.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#hostsEnvironment -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#hostsEnvironment">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tomensom.com/saso#HardwareEnvironment"/>
                    <rdf:Description rdf:about="http://tomensom.com/saso#SoftwareEnvironment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://tomensom.com/saso#SoftwareEnvironment"/>
        <rdfs:comment>A Technical Environment (Hardware or Software) may host another Software Environment.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://tomensom.com/saso#isExecutableIn -->

    <owl:ObjectProperty rdf:about="http://tomensom.com/saso#isExecutableIn">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#SoftwareSuperObject"/>
        <rdfs:range rdf:resource="http://tomensom.com/saso#TechnicalEnvironment"/>
        <rdfs:comment>A Software Super-Object may have one or more Technical Environment in which it can be sucessfully executed.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tomensom.com/saso#architecture -->

    <owl:DatatypeProperty rdf:about="http://tomensom.com/saso#architecture">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>Describes the processor or instruction set architecture that the software component is designed for.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://tomensom.com/saso#isExternallyHosted -->

    <owl:DatatypeProperty rdf:about="http://tomensom.com/saso#isExternallyHosted">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#AbstractComponent"/>
        <rdfs:domain rdf:resource="http://tomensom.com/saso#ConcreteComponent"/>
        <rdfs:comment>Indicates that a component is not maintained by the organisation.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://tomensom.com/saso#isVirtual -->

    <owl:DatatypeProperty rdf:about="http://tomensom.com/saso#isVirtual">
        <rdfs:domain rdf:resource="http://tomensom.com/saso#Hardware"/>
        <rdfs:comment>Indicates that a hardware component is virtual.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://tomensom.com/saso#version -->

    <owl:DatatypeProperty rdf:about="http://tomensom.com/saso#version">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://tomensom.com/saso#Data"/>
                    <rdf:Description rdf:about="http://tomensom.com/saso#Software"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment>The version number or code of a particular component.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tomensom.com/saso#API -->

    <owl:Class rdf:about="http://tomensom.com/saso#API">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:label>API</rdfs:label>
        <skos:altLabel>Application Programming Interface</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#AbstractComponent -->

    <owl:Class rdf:about="http://tomensom.com/saso#AbstractComponent">
        <rdfs:label>Abstract Component</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Android -->

    <owl:Class rdf:about="http://tomensom.com/saso#Android">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#OperatingSystem"/>
        <rdfs:label>Android</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Artwork -->

    <owl:Class rdf:about="http://tomensom.com/saso#Artwork">
        <rdfs:comment>A distinct intellectual creation.</rdfs:comment>
        <rdfs:label>Artwork</rdfs:label>
        <skos:altLabel>Work</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#AudioInterface -->

    <owl:Class rdf:about="http://tomensom.com/saso#AudioInterface">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ExternalHardware"/>
        <rdfs:label>Audio Interface</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Binary -->

    <owl:Class rdf:about="http://tomensom.com/saso#Binary">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>The executable representation of a software program.</rdfs:comment>
        <rdfs:label>Binary</rdfs:label>
        <skos:altLabel>Executable, Computer Program, Software Application, App</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#CPU -->

    <owl:Class rdf:about="http://tomensom.com/saso#CPU">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:label>CPU</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Case -->

    <owl:Class rdf:about="http://tomensom.com/saso#Case">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Hardware"/>
        <owl:disjointWith rdf:resource="http://tomensom.com/saso#ExternalHardware"/>
        <owl:disjointWith rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:label>Case</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#ConcreteComponent -->

    <owl:Class rdf:about="http://tomensom.com/saso#ConcreteComponent">
        <rdfs:label>Concrete Component</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Connector -->

    <owl:Class rdf:about="http://tomensom.com/saso#Connector">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>Software that allows communication between other software components.</rdfs:comment>
        <rdfs:label>Connector</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Controller -->

    <owl:Class rdf:about="http://tomensom.com/saso#Controller">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:comment>Hardware that provides an interface for other hardware to connect to the host machine.</rdfs:comment>
        <rdfs:label>Controller</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Data -->

    <owl:Class rdf:about="http://tomensom.com/saso#Data">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ConcreteComponent"/>
        <rdfs:comment>A component with a material manifestation which is only verifiable through the use of computer hardware and appropriate rendering software.</rdfs:comment>
        <rdfs:label>Data</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#DatabaseSoftware -->

    <owl:Class rdf:about="http://tomensom.com/saso#DatabaseSoftware">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>Software that manages databases.</rdfs:comment>
        <rdfs:label>Database Software</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#DisplayDevice -->

    <owl:Class rdf:about="http://tomensom.com/saso#DisplayDevice">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ExternalHardware"/>
        <rdfs:label>Display Device</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Driver -->

    <owl:Class rdf:about="http://tomensom.com/saso#Driver">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A specialised form of software which supports communication between software, operating system and hardware.</rdfs:comment>
        <rdfs:label>Driver</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#ExternalHardware -->

    <owl:Class rdf:about="http://tomensom.com/saso#ExternalHardware">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Hardware"/>
        <owl:disjointWith rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:comment>Hardware component which is intended to be exposed during use.</rdfs:comment>
        <rdfs:label>External Hardware</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#GPU -->

    <owl:Class rdf:about="http://tomensom.com/saso#GPU">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:label>GPU</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#HDD -->

    <owl:Class rdf:about="http://tomensom.com/saso#HDD">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#StorageDevice"/>
        <rdfs:label>HDD</rdfs:label>
        <skos:altLabel>Hard Disk Drive</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Hardware -->

    <owl:Class rdf:about="http://tomensom.com/saso#Hardware">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ConcreteComponent"/>
        <rdfs:comment>A component with a material manifestation which is verifiable without the use of other hardware.</rdfs:comment>
        <rdfs:label>Hardware</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#HardwareEnvironment -->

    <owl:Class rdf:about="http://tomensom.com/saso#HardwareEnvironment">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#TechnicalEnvironment"/>
        <rdfs:comment>A constellation of interconnected software components that form an environment in which software might be executed.</rdfs:comment>
        <rdfs:label>Hardware Environment</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Image -->

    <owl:Class rdf:about="http://tomensom.com/saso#Image">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Data"/>
        <rdfs:label>Image</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Instrument -->

    <owl:Class rdf:about="http://tomensom.com/saso#Instrument">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A specialised type of software which is capable of intercepting or measuring the properties of a hardware or software component.</rdfs:comment>
        <rdfs:label>Instrument</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#InternalHardware -->

    <owl:Class rdf:about="http://tomensom.com/saso#InternalHardware">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Hardware"/>
        <rdfs:comment>Hardware component which is intended to be enclosed during use.</rdfs:comment>
        <rdfs:label>Internal Hardware</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#InternalSoundcard -->

    <owl:Class rdf:about="http://tomensom.com/saso#InternalSoundcard">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:label>Internal Soundcard</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Keyboard -->

    <owl:Class rdf:about="http://tomensom.com/saso#Keyboard">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ExternalHardware"/>
        <rdfs:label>Keyboard</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Linux -->

    <owl:Class rdf:about="http://tomensom.com/saso#Linux">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#OperatingSystem"/>
        <rdfs:label>Linux</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#MacOS -->

    <owl:Class rdf:about="http://tomensom.com/saso#MacOS">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#OperatingSystem"/>
        <rdfs:label>MacOS</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Monitor -->

    <owl:Class rdf:about="http://tomensom.com/saso#Monitor">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#DisplayDevice"/>
        <rdfs:label>Monitor</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Mouse -->

    <owl:Class rdf:about="http://tomensom.com/saso#Mouse">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ExternalHardware"/>
        <rdfs:label>Mouse</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#OperatingSystem -->

    <owl:Class rdf:about="http://tomensom.com/saso#OperatingSystem">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A specialised form of software supporting the execution of software programs and communication with hardware and other components. An operating system is usually composed of a kernel—the primary control system—and supporting interfaces, frameworks and services.</rdfs:comment>
        <rdfs:label>Operating System</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Printer -->

    <owl:Class rdf:about="http://tomensom.com/saso#Printer">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ExternalHardware"/>
        <rdfs:label>Printer</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Projector -->

    <owl:Class rdf:about="http://tomensom.com/saso#Projector">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#DisplayDevice"/>
        <rdfs:label>Projector</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#RAM -->

    <owl:Class rdf:about="http://tomensom.com/saso#RAM">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#InternalHardware"/>
        <rdfs:label>RAM</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Realisation -->

    <owl:Class rdf:about="http://tomensom.com/saso#Realisation">
        <rdfs:comment>An embodiment of a particular variant of the work in time and space.</rdfs:comment>
        <rdfs:label>Realisation</rdfs:label>
        <skos:altLabel>Instance; Instantiation, Manifestation</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#RuntimeEnvironment -->

    <owl:Class rdf:about="http://tomensom.com/saso#RuntimeEnvironment">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>Software that provides an environment in which other software can be executed.</rdfs:comment>
        <rdfs:label>Runtime Environment</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#RuntimeLibrary -->

    <owl:Class rdf:about="http://tomensom.com/saso#RuntimeLibrary">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
