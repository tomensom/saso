        <rdfs:label>Runtime Library</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#SQL -->

    <owl:Class rdf:about="http://tomensom.com/saso#SQL">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Data"/>
        <rdfs:label>SQL</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#SSD -->

    <owl:Class rdf:about="http://tomensom.com/saso#SSD">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#StorageDevice"/>
        <rdfs:label>SSD</rdfs:label>
        <skos:altLabel>Solid State Drive</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#SSHD -->

    <owl:Class rdf:about="http://tomensom.com/saso#SSHD">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#StorageDevice"/>
        <rdfs:label>SSHD</rdfs:label>
        <skos:altLabel>Solid State Hybrid Drive</skos:altLabel>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#Software -->

    <owl:Class rdf:about="http://tomensom.com/saso#Software">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#ConcreteComponent"/>
        <rdfs:comment>A component with a material manifestation which is only verifiable through the use of computer hardware.</rdfs:comment>
        <rdfs:label>Software</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#SoftwareEnvironment -->

    <owl:Class rdf:about="http://tomensom.com/saso#SoftwareEnvironment">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#TechnicalEnvironment"/>
        <rdfs:comment>A constellation of interconnected hardware components that form an environment in which software might be executed.</rdfs:comment>
        <rdfs:label>Software Environment</rdfs:label>
    </owl:Class>
    


    <!-- http://tomensom.com/saso#SoftwareSuperObject -->

    <owl:Class rdf:about="http://tomensom.com/saso#SoftwareSuperObject">
        <rdfs:subClassOf rdf:resource="http://tomensom.com/saso#Software"/>
        <rdfs:comment>A subset of software consisting of binaries and data assets which perform some function or purpose. This component is a simplification of what may be a very variable structure.</rdfs:comment>
